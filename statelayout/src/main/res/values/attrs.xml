<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StateLayout">
        <attr name="statelayout_loading_view" format="reference"/>
        <attr name="statelayout_empty_view" format="reference"/>
        <attr name="statelayout_failure_view" format="reference"/>
        <attr name="statelayout_default_view" format="enum">
            <enum name="loading_state" value="0"/>
            <enum name="failure_state" value="1"/>
            <enum name="empty_state" value="2"/>
        </attr>
    </declare-styleable>
</resources>